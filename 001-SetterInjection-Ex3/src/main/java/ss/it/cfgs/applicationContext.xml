<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
    
    <!-- bean id configuration -->
    <bean id="uid" class="ss.it.beans.UIDAI">
    <property name="uidno" value="541234532412"/> <!-- setter injection -->
    <property name="name" value="A"/> <!-- setter injection -->
    <property name="dob" value="15061994"/> <!-- setter injection -->
    <property name="add" value="Hyderabad"/> <!-- setter injection -->
    </bean>
    
    
    </beans>